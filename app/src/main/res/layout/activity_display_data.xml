					android:layout_height="wrap_content"
					android:textAppearance="?android:attr/textAppearanceMedium"
					android:text="0"
					android:id="@+id/riders_out_8"
					android:layout_row="8"
					android:layout_column="1"
					android:layout_marginLeft="20dp"/>

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textAppearance="?android:attr/textAppearanceMedium"
					android:text="0"
					android:id="@+id/riders_out_9"
					android:layout_row="9"
					android:layout_column="1"
					android:layout_marginLeft="20dp"/>

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textAppearance="?android:attr/textAppearanceMedium"
					android:text="0"
					android:id="@+id/riders_out_10"
					android:layout_row="10"
					android:layout_column="1"
					android:layout_marginLeft="20dp"/>

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textAppearance="?android:attr/textAppearanceMedium"
					android:text="0"
					android:id="@+id/riders_out_11"
					android:layout_row="11"
					android:layout_column="1"
					android:layout_marginLeft="20dp"/>

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textAppearance="?android:attr/textAppearanceMedium"
					android:text="0"
					android:id="@+id/riders_out_12"
					android:layout_row="12"
					android:layout_column="1"
					android:layout_marginLeft="20dp"/>

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textAppearance="?android:attr/textAppearanceMedium"
					android:text="0"
					android:id="@+id/riders_out_13"
					android:layout_row="13"
					android:layout_column="1"
					android:layout_marginLeft="20dp"/>

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textAppearance="?android:attr/textAppearanceMedium"
					android:text="0"
					android:id="@+id/riders_out_14"
					android:layout_row="14"
					android:layout_column="1"
					android:layout_marginLeft="20dp"/>

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textAppearance="?android:attr/textAppearanceMedium"
					android:text="0"
					android:id="@+id/riders_out_15"
					android:layout_row="15"
					android:layout_column="1"
					android:layout_marginLeft="20dp"/>

			</GridLayout>

			<LinearLayout
				android:orientation="horizontal"
				android:layout_width="fill_parent"
				android:layout_height="fill_parent"
				android:id="@+id/linear_totalcycles"
				android:layout_marginTop="10dp"
				>

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textAppearance="?android:attr/textAppearanceLarge"
					android:text="Total Cycles: "
					android:id="@+id/textView"/>

				<TextView
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:textAppearance="?android:attr/textAppearanceLarge"
					android:text="0"
					android:id="@+id/total_cycles"
					android:gravity="right"
					android:layout_marginRight="30dp"/>
			</LinearLayout>

			<LinearLayout
				android:orientation="horizontal"
				android:layout_width="fill_parent"
				android:layout_height="fill_parent"
				android:id="@+id/linearLayout"
				android:layout_marginTop="10dp">

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textAppearance="?android:attr/textAppearanceLarge"
					android:text="Total FastLane:"
					android:id="@+id/textView4"/>

				<TextView
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:textAppearance="?android:attr/textAppearanceLarge"
					android:text="0"
					android:id="@+id/total_fastlane"
					android:layout_marginLeft="0dp"
					android:gravity="right"
					android:layout_marginRight="30dp"/>
			</LinearLayout>

			<LinearLayout
				android:orientation="horizontal"
				android:layout_width="fill_parent"
				android:layout_height="fill_parent"
				android:id="@+id/linear_totalriders"
				android:layout_marginTop="10dp">

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textAppearance="?android:attr/textAppearanceLarge"
					android:text="Total Riders:"
					android:id="@+id/textView3"/>

				<TextView
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:textAppearance="?android:attr/textAppearanceLarge"
					android:text="0"
					android:id="@+id/total_riders"
					android:layout_marginLeft="10dp"
					android:layout_marginRight="30dp"
					android:gravity="right"/>
			</LinearLayout>

		</LinearLayout>
	</ScrollView>
</RelativeLayout>
