				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="9 - 10"
					android:id="@+id/time_9_10pm"
					android:layout_row="13"
					android:layout_column="0"
					android:padding="5dp"/>

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="10 - 11"
					android:id="@+id/time_10_11pm"
