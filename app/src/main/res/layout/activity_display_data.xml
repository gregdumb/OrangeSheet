					android:layout_column="0"
					android:padding="5dp"/>

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="1 - 2"
					android:id="@+id/time_1_2"
					android:layout_row="5"
					android:layout_column="0"
					android:padding="5dp"/>

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="2 - 3"
					android:id="@+id/time_2_3"
					android:layout_row="6"
					android:layout_column="0"
					android:padding="5dp"/>

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="3 - 4"
					android:id="@+id/time_3_4"
